        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mmarquezpr.datademo.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mmarquezpr.datademo.app.DataActivity"
            android:label="@string/title_activity_data" >
        </activity>
        <activity
            android:name="mmarquezpr.datademo.app.SharedPrefActivity"
            android:label="@string/title_activity_shared_pref" >
        </activity>
    </application>

</manifest>
